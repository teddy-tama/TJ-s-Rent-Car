<%- include('_partials/head') %>
    <%- include('_partials/navbar') %>
    <div class="container">
      <h1 class='mt-3'><center>Welcome!</center></h1>
        <div class="row">
        <% data.forEach(el=>{%>
          <div class="col my-5">
            <div class="card" style="width: 18rem;">
              <img src="public/img/<%= el.image %>" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title"><%= el.type %></h5>
                <p class="card-text"><%= el.detail %>.</p>
              </div>
              <ul class="list-group list-group-flush">
                <li class="list-group-item">Harga: Rp. <%=el.price%></li>
                <li class="list-group-item">Penyewa: <%=el.Admin ? el.Admin.full_name : 'unknown' %></li>
                <li class="list-group-item">Status: <%= el.status %></li>
              </ul>
              <div class="card-body">
                <a href="#" class="card-link btn-link <%= el.status!=='Belum Disewakan' ? 'disabled' : '' %>" >Sewa</a>
                <a href="#" class="card-link">Edit</a>
                <a href="#" class="card-link">Hapus</a>
              </div>
            </div>
          </div>
          <%})%>
        </div>
      </div>
    <%- include('_partials/script') %>